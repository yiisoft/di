{
    "name": "yiisoft/di",
    "type": "library",
    "description": "Yii DI container",
    "keywords": [
        "di",
        "dependency",
        "injection",
        "container",
        "injector",
        "autowiring",
        "psr-11"
    ],
    "homepage": "http://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/di/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/di"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "ext-mbstring": "*",
        "psr/container": "1.0.0",
        "yiisoft/factory": "^3.0@dev",
        "yiisoft/injector": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.18.0",
        "vimeo/psalm": "^3.16",
        "phpunit/phpunit": "^9.3",
        "phpbench/phpbench": "~0.17",
        "league/container": "^3.2"
    },
    "suggest": {
        "yiisoft/injector": "^1.0@dev",
        "phpbench/phpbench": "To run benchmarks."
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Di\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Di\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "phpunit --coverage-html coverage"
        ]
    },
    "scripts-descriptions": {
        "test": "Run all tests"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
