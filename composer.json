{
    "name": "yiisoft/di",
    "type": "library",
    "description": "Yii DI container",
    "keywords": [
        "di",
        "dependency",
        "injection",
        "container",
        "injector",
        "autowiring",
        "psr-11"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/di/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "ircs://irc.libera.chat:6697/yii",
        "chat": "https://t.me/yii3en",
        "source": "https://github.com/yiisoft/di"
    },
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "psr/container": "^1.1|^2.0",
        "yiisoft/definitions": "dev-parameter-name-binding"
    },
    "require-dev": {
        "league/container": "^4.2",
        "maglnet/composer-require-checker": "^4.2",
        "phpbench/phpbench": "^1.2.0",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.18.3",
        "roave/infection-static-analysis-plugin": "^1.25",
        "spatie/phpunit-watcher": "^1.23",
        "vimeo/psalm": "^4.30|^5.7",
        "yiisoft/injector": "^1.0",
        "yiisoft/test-support": "^3.0"
    },
    "suggest": {
        "yiisoft/injector": "^1.0",
        "phpbench/phpbench": "To run benchmarks."
    },
    "provide": {
        "psr/container-implementation": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Di\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Di\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --testdox --no-interaction",
        "test-watch": "phpunit-watcher watch"
    },
    "scripts-descriptions": {
        "test": "Run all tests"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true
        }
    }
}
